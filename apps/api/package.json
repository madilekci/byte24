{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "prisma": {
    "schema": "./prisma/schema",
    "seed": "node prisma/seed.ts"
  },
  "scripts": {
    "prepare": "husky",
    "db:migrate:new": "npx prisma migrate dev --name",
    "db:migrate": "npx prisma migrate dev && npx prisma generate",
    "db:seed": "ts-node prisma/seed.ts",
    "db:reset": "npx prisma migrate reset --force init",
    "db:generate": "npx prisma generate",
    "db:push": "npx prisma db push",
    "seed": "ts-node prisma/seed.ts",
    "build": "npx prisma generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "yarn start:dev",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "email": "email dev --dir ./src/mail/templates --port 3002",
    "prisma:format": "npx prisma format",
    "prisma:update-package": "yarn add @prisma/client@latest && yarn add @prisma/client@latest --dev",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org byte24-bv --project lavish-backend ./dist && sentry-cli sourcemaps upload --org byte24-bv --project lavish-backend ./dist"
  },
